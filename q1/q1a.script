#!/bin/bash
#SBATCH --verbose
#SBATCH --nodes=1
#SBATCH --time=0:10:00
#SBATCH --job-name="aliu_q1a"
#SBATCH --gres=gpu:p100:1
#SBATCH --partition=courses-gpu

VALUES=(4096 16384 65536 262144 1048576)

# Loop over arguments
for N in "${VALUES[@]}"; do
    OUTPUT_FILE="q1a_${N}.out"  
    ./q1a $N > $OUTPUT_FILE 2>&1
done